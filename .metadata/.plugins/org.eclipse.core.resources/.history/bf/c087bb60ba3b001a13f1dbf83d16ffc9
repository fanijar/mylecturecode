
public class ForLoopAndIfElseBlock {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int[] number = {1, 5, 7, 3};
		// find in array number ?? and print name on console
		
		int checkNumber = 5;
		if (number[1] == checkNumber){
			System.out.println("FIVE");
			
		}else{
			System.out.println("No match");
		}
		

	}

}
