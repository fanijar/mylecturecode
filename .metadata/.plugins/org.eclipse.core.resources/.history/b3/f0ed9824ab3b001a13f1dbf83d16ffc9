import java.util.Arrays;


public class ArraysAndLoop {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// declare variable
		int[] marks = {76, 23, 42, 100, 99, 10, 56};
		// Make toString all elements of array
		//print in console
		System.out.println(Arrays.toString(marks));

	}

}
