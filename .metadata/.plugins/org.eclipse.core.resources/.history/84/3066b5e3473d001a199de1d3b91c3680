		System.out.println("Number 2 is: ");
		int number2 = scanner.nextInt();
		
		int num1 = 55;
		int num2 = 60;
		char add = '+';
		int result = num1 + num2;
		
		
		
		//syso(result = num1 + num2);

	}

}
