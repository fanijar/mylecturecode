
public class TaskVar {

	/**
	 * numAdd = 125 - 24;
		numAdd = 125 * 24;
		numAdd = 125 / 24;
		numAdd = 125 % 24;
	 */
	public static void main(String[] args) {
		int numAdd;
		numAdd = 125 + 24;
		
		System.out.println(numAdd + " = 125 + 24");

		int numAdd1;
		numAdd1 = 125 - 24;
		
		System.out.println(numAdd1 + " = 125 - 24");
		
		int numAdd3;
		numAdd3 = 125 * 24;
		
		System.out.println(numAdd3 + " = 125 * 24");
		
		int numAdd4;
		numAdd4 = 125 / 24;
		
		System.out.println(numAdd4 + " = 125  / 24");
		
		int numAdd5;
		numAdd5 = 125 % 24;
		
		System.out.println(numAdd5 + " = 125 % 24");
	}

}
